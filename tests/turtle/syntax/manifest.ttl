        :turtle-star-bnode-1
        :turtle-star-bnode-2
        :turtle-star-bnode-3

        :turtle-star-bad-1
        :turtle-star-bad-2
        :turtle-star-bad-3
        :turtle-star-bad-4
        :turtle-star-bad-5
        :turtle-star-bad-6
        :turtle-star-bad-7
