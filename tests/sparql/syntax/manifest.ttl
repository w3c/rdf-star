    mf:action    <sparql-star-annotation-2.ttl> ;
    .

:sparql-star-ann-3 rdf:type mf:PositiveSyntaxTest ;
    mf:name      "SPARQL* - Annotation example" ;
    mf:action    <sparql-star-annotation-3.ttl> ;
    .

