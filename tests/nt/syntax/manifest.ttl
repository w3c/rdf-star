PREFIX rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs:   <http://www.w3.org/2000/01/rdf-schema#>
PREFIX mf:     <http://www.w3.org/2001/sw/DataAccess/tests/test-manifest#>
PREFIX rdft:   <http://www.w3.org/ns/rdftest#>
PREFIX :       <#>

<>  rdf:type mf:Manifest ;
    rdfs:label "N-Triples* Syntax Tests" ;
    mf:entries
    (
        :ntriples-star-1
        :ntriples-star-2
        :ntriples-star-3

        :ntriples-star-bnode-1
        :ntriples-star-bnode-2

        :ntriples-star-nested-1
        :ntriples-star-nested-2

        :ntriples-star-bad-1
        :ntriples-star-bad-2
        :ntriples-star-bad-3
    ) .

## Good Syntax

:ntriples-star-1 rdf:type rdft:TestTurtlePositiveSyntax ;
   mf:name      "N-Triples* - subject triple term" ;
   mf:action    <ntriples-star-syntax-1.nt> ;
   .

:ntriples-star-2 rdf:type rdft:TestTurtlePositiveSyntax ;
   mf:name      "N-Triples* - object triple term" ;
   mf:action    <ntriples-star-syntax-2.nt> ;
   .

:ntriples-star-3 rdf:type rdft:TestTurtlePositiveSyntax ;
   mf:name      "N-Triples* - subject and object triple terms" ;
   mf:action    <ntriples-star-syntax-3.nt> ;
   .

# Blank nodes

:ntriples-star-bnode-1 rdf:type rdft:TestTurtlePositiveSyntax ;
   mf:name      "N-Triples* - Blank node subject" ;
   mf:action    <ntriples-star-bnode-1.nt> ;
   .

:ntriples-star-bnode-2 rdf:type rdft:TestTurtlePositiveSyntax ;
   mf:name      "N-Triples* - Blank node object" ;
   mf:action    <ntriples-star-bnode-2.nt> ;
   .
   
:ntriples-star-nested-1 rdf:type rdft:TestTurtlePositiveSyntax ;
   mf:name      "N-Triples* -  Nested subject term" ;
   mf:action    <ntriples-star-nested-1.nt> ;
   .

:ntriples-star-nested-2 rdf:type rdft:TestTurtlePositiveSyntax ;
   mf:name      "N-Triples* - Nested object term" ;
   mf:action    <ntriples-star-nested-2.nt> ;
   .

## Bad syntax

 
:ntriples-star-bad-1 rdf:type rdft:TestTurtleNegativeSyntax ;
    mf:name      "N-Triples* - Bad - triple term as predicate" ;
    mf:action    <ntriples-star-syntax-bad-1.ttl> ;
    .
    
:ntriples-star-bad-2 rdf:type rdft:TestTurtleNegativeSyntax ;
    mf:name      "N-Triples* - Bad - triple term, literal subject" ;
    mf:action    <ntriples-star-syntax-bad-2.ttl> ;
    .

:ntriples-star-bad-3 rdf:type rdft:TestTurtleNegativeSyntax ;
    mf:name      "N-Triples* - Bad - triple term, literal predicate" ;
    mf:action    <ntriples-star-syntax-bad-3.ttl> ;
    .

:ntriples-star-bad-4 rdf:type rdft:TestTurtleNegativeSyntax ;
    mf:name      "N-Triples* - Bad - triple term, blank node predicate" ;
    mf:action    <ntriples-star-syntax-bad-4.ttl> ;
    .
