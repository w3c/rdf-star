:turtle-star-1 rdf:type rdft:TestTurtleEval ;
   mf:name      "Turtle* - subject triple term" ;
   mf:action    <turtle-star-eval-01.ttl> ;
   mf:result    <turtle-star-eval-01.nt> ;
   .

:turtle-star-2 rdf:type rdft:TestTurtleEval ;
