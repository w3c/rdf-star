   .

:nt-ttl-star-5 rdf:type rdft:TestNTriplesPositiveSyntax ;
   mf:name      "N-Triples-star as Turtle-star - Nested, no whitespace" ;
   mf:action    <ntriples-star-syntax-5.nt> ;
   .

