<>  rdf:type mf:Manifest ;
    rdfs:label "Turtle-star Syntax Tests" ;
    mf:entries
    (
        :turtle-star-1
        :turtle-star-2

        :turtle-star-inside-1
        :turtle-star-inside-2

        :turtle-star-nested-1
        :turtle-star-nested-2

